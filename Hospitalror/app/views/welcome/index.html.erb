<h1>Patient Database </h1>
<%= link_to "Create Patient", new_patient_path %>
<ul> Patients
  <% @patients.each do |patient| %>
    <li>
      <%= patient.first_name %> | <%= patient.last_name %> [ <%= link_to "Edit", edit_patient_path(patient) %> | <%= link_to "Delete", patient_path(patient), method: :delete %> ]
    </li>
  <% end %>
</ul>


